{
  "name": "example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "tsc": "tsc",
    "clean": "rimraf artifacts",
    "build": "yarn run clean && yarn run tsc",
    "watch": "yarn run build -w",
    "start:ios": "yarn run build && concurrently -r 'yarn run watch' 'react-native run-ios'",
    "start:android": "yarn run build && concurrently -r 'yarn run watch' 'react-native run-android'",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.3.1",
    "react-native": "0.55.3"
  },
  "devDependencies": {
    "@types/react": "^16.3.13",
    "@types/react-native": "^0.55.10",
    "babel-jest": "22.4.3",
    "babel-preset-react-native": "4.0.0",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "concurrently": "^3.5.1",
    "jest": "22.4.3",
    "react-native-i18n-ts": "^0.1.0",
    "react-test-renderer": "16.3.1",
    "rimraf": "^2.6.2",
    "ts-jest": "^21.0.1",
    "typescript": "^2.6.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
