{
  "name": "redux-rn-misc-enhancer",
  "version": "0.1.0",
  "description": "A light weight wrapper of AppState and NetInfo which connects those changes directly to your Redux store.",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "repository": "https://github.com/quipper/redux-rn-misc-enhancer",
  "bugs": "https://github.com/quipper/redux-rn-misc-enhancer/issues",
  "keywords": [
    "react-native",
    "redux"
  ],
  "author": "Shintaro Katafuchi",
  "license": "Apache-2.0",
  "scripts": {
    "tsc": "tsc",
    "test": "jest"
  },
  "peerDependencies": {
    "react": "^16.4.1",
    "react-native": "^0.56.0",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^23.1.6",
    "@types/react": "^16.4.6",
    "@types/react-native": "^0.56.1",
    "@types/redux": "^3.6.0",
    "@types/redux-mock-store": "^1.0.0",
    "jest": "^23.4.1",
    "prettier": "^1.13.7",
    "react-native": "^0.56.0",
    "redux-mock-store": "^1.5.3",
    "ts-jest": "^23.0.1",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "typescript": "^2.9.2"
  },
  "jest": {
    "preset": "react-native",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(Test))\\.(jsx?|tsx?)$",
    "testPathIgnorePatterns": [
      "<rootDir>/build/",
      "<rootDir>/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
